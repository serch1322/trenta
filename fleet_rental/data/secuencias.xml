<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="secuencia_renta_de_carro" model="ir.sequence">
            <field name="name">Secuencia Renta de Carro</field>
            <field name="code">secuencia.renta.de.carro</field>
            <field name="prefix">RENTA/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">4</field>
        </record>

        <record id="secuencia_vehiculos" model="ir.sequence">
            <field name="name">Secuencia Vehiculos</field>
            <field name="code">secuencia.vehiculos</field>
            <field name="prefix">TR-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

        <record id="secuencia_aditamentos" model="ir.sequence">
            <field name="name">Secuencia Aditamentos</field>
            <field name="code">secuencia.aditamentos</field>
            <field name="prefix">ATR-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

    </data>

</odoo>